{
  "name": "@openzeppelin/defender-sdk-code-inspector-client",
  "version": "1.13.4",
  "description": "Client library for managing Defender Code Inspector",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "scripts": {
    "build": "rm -rf lib && tsc",
    "test": "npm run test:unit",
    "test:unit": "jest --verbose --passWithNoTests --forceExit",
    "watch": "tsc -w",
    "prepare": "npm run build",
    "test:clean": "jest --clearCache"
  },
  "files": [
    "lib",
    "!*.test.js",
    "!*.test.js.map",
    "!*.test.d.ts",
    "!*__mocks__"
  ],
  "author": "OpenZeppelin Defender <defender@openzeppelin.com>",
  "license": "MIT",
  "dependencies": {
    "@openzeppelin/defender-sdk-base-client": "^1.13.4",
    "axios": "^1.7.2",
    "lodash": "^4.17.21"
  },
  "publishConfig": {
    "access": "public"
  }
}